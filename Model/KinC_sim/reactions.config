
# Transcription
->KinC		|	2*viKinC/(2**(((t-trep)/6)%1+trep/6))
->Spo0A		|	(v0A0+v0Amax*(s)/(K0A+s))
->Spo0F		|	(v0F0+v0Fmax*(s**2)/(K0F**2+s**2))*2/(2**(((t)%6)/6))
->Spo0B		|	0.7
->Rap		|	0.105	
->Spo0E		|	0.07

# ->PspoIIg	|	

# Delay analog 

->s	|		0.7*(Spo0A_p-s)



# Autophosphorelation

KinC->KinC_p		| kap*KinC
KinC_p->KinC		| kadp*KinC_p

# Phosphorelay reactions

KinC_p + Spo0F ->   KinC_p_Spo0F		|	kb*KinC_p*Spo0F
KinC_p_Spo0F   ->   KinC_p + Spo0F 		|	k1*KinC_p_Spo0F

KinC_p_Spo0F   ->   KinC + Spo0F_p		|	k2*KinC_p_Spo0F
KinC + Spo0F_p ->   KinC_p_Spo0F   		|	kb*KinC*Spo0F_p

	
Spo0F_p + Spo0B -> Spo0F_p_Spo0B		|	kb*Spo0F_p*Spo0B
Spo0F_p_Spo0B   -> Spo0F_p + Spo0B 		|	k3*Spo0F_p_Spo0B

Spo0F_p_Spo0B   ->  Spo0F + Spo0B_p		|	k4*Spo0F_p_Spo0B
Spo0F + Spo0B_p ->  Spo0F_p_Spo0B		|	kb*Spo0F*Spo0B_p


Spo0B_p + Spo0A -> Spo0B_p_Spo0A		|	kb*Spo0B_p*Spo0A
Spo0B_p_Spo0A   -> Spo0B_p + Spo0A 		|	k5*Spo0B_p_Spo0A

Spo0B_p_Spo0A   -> Spo0A_p + Spo0B		|	k6*Spo0B_p_Spo0A
Spo0A_p + Spo0B -> Spo0B_p_Spo0A		|	kb*Spo0B*Spo0A_p

KinC_p + Spo0A -> KinC_p_Spo0A			|	kb*KinC_p*Spo0A
KinC_p_Spo0A -> KinC_p + Spo0A			|	kd1*KinC_p_Spo0A

KinC_p_Spo0A -> Spo0A_p+KinC			|	kd2*KinC_p_Spo0A
Spo0A_p+KinC-> KinC_p_Spo0A			|	kb*Spo0A_p*KinC


# Feedback repression

# Spo0F+KinC -> Spo0F_KinC			|	kb*Spo0F*KinC
# Spo0F_KinC  -> Spo0F+KinC			|	ki*Spo0F_KinC


# Repression of 0E Rap

Spo0F_p + Rap  ->  Spo0F_p_Rap			|	kb*Spo0F_p*Rap
Spo0F_p_Rap  ->  Spo0F_p + Rap			|	kubr*Spo0F_p_Rap
Spo0F_p_Rap  ->  Spo0F + Rap			|	kr*Spo0F_p_Rap

Spo0A_p + Spo0E  ->  Spo0A_p_Spo0E			|	kb*Spo0A_p*Spo0E
Spo0A_p_Spo0E  ->  Spo0A_p + Spo0E			|	kube*Spo0A_p_Spo0E
Spo0A_p_Spo0E  ->  Spo0A + Spo0E			|	ke*Spo0A_p_Spo0E

# Degradation

KinC 		->	  |	   kdeg*KinC 			
KinC_p	       	->	  |	   kdeg*KinC_p	       		 
KinC_p_Spo0F   	->	  |	   kdeg*KinC_p_Spo0F   		 
Rap	       	->	  |	   kdeg*Rap	       		 
Spo0A          	->	  |	   kdeg*Spo0A          		
Spo0A_p        	->	  |	   kdeg*Spo0A_p        		
Spo0A_p_Spo0E  	->	  |	   kdeg*Spo0A_p_Spo0E  		
Spo0B          	->	  |	   kdeg*Spo0B          		
Spo0B_p        	->	  |	   kdeg*Spo0B_p        		
Spo0B_p_Spo0A  	->	  |	   kdeg*Spo0B_p_Spo0A  		
Spo0E          	->	  |	   kdeg*Spo0E          		 
Spo0F          	->	  |	   kdeg*Spo0F          		
# Spo0F_KinC     	->	  |	   kdeg*Spo0F_KinC     		
Spo0F_p        	->	  |	   kdeg*Spo0F_p        		
Spo0F_p_Rap    	->	  |	   kdeg*Spo0F_p_Rap    		
Spo0F_p_Spo0B  	->	  |	   kdeg*Spo0F_p_Spo0B 
KinC_p_Spo0A    ->	  |	   kdeg*KinC_p_Spo0A		



###########################################################


ss->			|	kdeg*ss
pr->			|	kdeg*pr
ps->			|	kdeg*ps
# ss accumulation model

->ss | float((Spo0A_p<uh)&(Spo0A_p>lh))*vs0

->pr |float(ss>sh)*vpr0

->ps |float(Spo0A_p>psh)*vps0
